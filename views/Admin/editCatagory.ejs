<%-include('./menu/header')%>
    <link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
    <script src="https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js"></script>
    <center>
        <h3>Edit Catagory</h3>
    </center>
    <hr>
    <form role="form" action="/admin/catagory/addCatagory/<%=catagoryData._id%>" method="POST">
        <div class="row">
            <div class="col-md-6 col-sm-6 col-xs-6">
                <label>Catagory :</label>
                <input class="form-control" value="<%=catagoryData.catagory%>" type="text" name="catagory" placeholder="Enter Catagory"
                    required>
            </div>
            <div class="col-md-6 col-sm-6 col-xs-6">
                <label>Status :</label>
                <br>
                <input type="checkbox" data-toggle="toggle" name="status" data-on="Approved" data-off="DisApprove" hidden <%if(catagoryData.status=='1'
                    ){%> checked
                <%}%>>
            </div>
        </div>
        </div>
        <br><hr>
        <div align="center">
            <input type="submit" class="btn btn-info" value="Update Catagory">
        </div>
    </form>
    <script>
        $(function () {
            $('#toggle-two').bootstrapToggle({
                on: 'Enabled',
                off: 'Disabled'
            });
        })
    </script>
    <%-include('./menu/footer')%>